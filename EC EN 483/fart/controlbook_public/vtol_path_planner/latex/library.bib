@article{KimSukkarieh04,
	abstract = {We address the issue of autonomous navigation, that is, the

ability for a navigation system to provide information about the

states of a vehicle without the need for a priori infrastructure

such as GPS, beacons, or a map. The algorithm is known as

simultaneous localisation and mapping (SLAM) and it is a terrain

aided navigation system (TANS) which has the capability for

online map building, and simultaneously utilising the generated

