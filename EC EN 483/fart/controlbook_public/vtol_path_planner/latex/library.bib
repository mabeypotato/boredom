@inproceedings{WilliamsTrivailo05,
	abstract = {When a long cable is towed in a circular flight-path, the system can
exhibit quasi-stationary solutions for which the cable tip appears
to remain stationary relative to the orbiting aircraft. For applications
involving pickup and delivery of payloads, tighter turns at high
speeds lead to nearly stationary motion of the cable tip in an inertial
frame. This work studies the dynamics of such a system, forcusing
on the stability and equilibria of solutions. The observation thta
higher drag on the towed body leads to smaller orbit radii of the
towed body suggest that utilizing a high drag device roughly midway
along the cable can enhance the overall performance of the system.
A numerical analysis of the modified system is carried out using
a discretized lumped mass model of the cable. By optimally placing
the drag device, improvements in the radius of the towed body can
be made.},
	author = {Williams, Paul and Trivailo, Pavel},
