	file = {:Users/beard/Dropbox/library/ForsterCarloneDellaert17-1.pdf:pdf},
	isbn = {9781467380256},
	issn = {15523098},
	journal = {IEEE Transactions on Robotics},
	number = {1},
	pages = {1--21},
	pmid = {15523098},
	title = {On-Manifold Preintegration for Real-Time},
