@article{LafferrierEtAl05,
	abstract = {This paper investigates a method for decentralized stabilization of
vehicle formations using techniques from algebraic graph theory.
The vehicles exchange information according to a pre-specified communication
digraph, G. A feedback control is designed using relative information
between a vehicle and its in-neighbors in G. We prove that a necessary
