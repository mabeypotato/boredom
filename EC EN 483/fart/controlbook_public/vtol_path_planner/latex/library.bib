@article{MarantosKarrasVlantis18,
	abstract = {This paper presents the design of a complete control
system for the autonomous landing of unmanned flybarless
helicopters on known stationary visual landmarks. A
state estimator based on the complementary filters notion,
estimates the position, translational velocity and attitude
vectors of the vehicle by fusing data acquired from the on--
board camera and an Inertial Measurement Unit. A visionaided
nonlinear model predictive controller is designed for
the landing motion of the helicopter, assuming that the
on--board camera is rigidly (i.e., no additional Degrees of
Freedom (DOF)) attached on the vehicle. Although the
under--actuated character of the helicopter dynamics introduces
counter--goals for minimizing the error between the
vehicle and the landmark, the proposed control scheme
guarantees, via hard nonlinear constraints, that the landmark
will always be kept inside the camera field of view during
the landing procedure. In order to simplify the derived algorithm
without violating the robustness of the proposed controller,
we reformulate the translational helicopter dynamics
in order to reduce the number of the unknown model parameters
to a minimum. Moreover, a parameter/disturbance
observer is designed for estimating simultaneously the vehicle's
unknown dynamic parameters as well as the induced
disturbances. The efficacy of the proposed landing scheme
is evaluated via a set of experimental and simulation results,
using a small--scale flybarless helicopter.},
	annote = {Vision based landing of a helicopter.
Ground target is an aruco marker.
Vision based state estimation, using aruco marker
Disturbance observer to get control gain and disturbances like drag.
MPC used to control helicopter and guarantee FOV constraints
Implemented in ROS / Gazebo.
Flight tests.
},
	author = {Panos Marantos and George C. Karras and Panagiotis Vlantis and Kostas J. Kyriakopoulos},
	date-added = {2018-10-05 15:46:48 -0600},
	date-modified = {2018-10-05 15:50:19 -0600},
	journal = {Journal of Intelligent and Robotic Systems},
	pages = {145--158},
	title = {Vision-based Autonomous Landing Control for Unmanned Helicopters},
	volume = {92},
	year = {2018},
