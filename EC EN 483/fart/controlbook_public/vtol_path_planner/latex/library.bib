	howpublished = {https://arxiv.org/abs/1807.02931},
	keywords = {B-spline, trajectory planning, collision avoidance, optimization, Three-dimensional trajectory planning, B-spline curves, disjunctive programming, receding horizon, obstacle avoidance.},
	title = {Optimal Trajectory-Planning of UAVs via B-Splines and Disjunctive Programming},
