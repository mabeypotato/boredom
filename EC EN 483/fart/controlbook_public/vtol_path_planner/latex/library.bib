@article{MettlerDadkhahKong10,
	abstract = {This paper describes an autonomous guidance system based on receding horizon (RH) optimization.
The system is integrated around a spatial, state-dependent cost-to-go (SVF) function that is computed
as an approximation to the value function associated with the optimal trajectory planning problem.
The function captures the critical interaction between the vehicle dynamics and environment, thereby
resulting in tighter coupling between planning and control. The consistency achieved between the RH
optimization and the SVF enables a more rigorous implementation of the RH framework to autonomous
vehicle guidance. The paper describes the overall approach along flight experimental results obtained in
an Interactive Guidance and Control Laboratory.},
	annote = {This paper addresses the problem of optimal guidance of small and agile unmanned vehicles in cluttered and possibly dynamic terrain. Optimal solutions of this problem are computationally intractable and so the paper proposes the use of receding horizon optimization with the specification of a cost-to-go calculation on the final state. The contribution of the paper is to propose a clever method for approximating the cost-to-go using the motion primitive automaton developed by Dr. Mettler{\&}{\#}039;s post doc advisors at MIT, for which she has also made unique contributions. I really enjoyed reading this paper. The problem was stated very clearly, the existing literature was explained and put in context, and the key contributions of the paper were clear and interesting. This paper and the many conference and journal papers associated with it represent a substantial contribution to the optimal guidance literature when real-time operation is required.},
	author = {Mettler, B and Dadkhah, N and Kong, Z},
	file = {:Users/beard/Library/Application Support/Mendeley Desktop/Downloaded/Mettler, Dadkhah, Kong - 2010 - Agile autonomous guidance using spatial value functions.pdf:pdf},
	journal = {Control Engineering Practice},
	keywords = {agile guidance,cost to go,model predictive control,receding horizon control},
