@article{SanyalLeeLeok08,
	abstract = {A deterministic attitude estimation problem for a rigid body in a potential field, with bounded attitude and angular velocity measurement
errors is considered. An attitude estimation algorithm that globally minimizes the attitude estimation error is obtained. Assuming that the initial
attitude, the initial angular velocity and measurement noise lie within given ellipsoidal bounds, an uncertainty ellipsoid that bounds the attitude
and the angular velocity of the rigid body is obtained. The center of the uncertainty ellipsoid provides point estimates, and the size of the
uncertainty ellipsoid measures the accuracy of the estimates. The point estimates and the uncertainty ellipsoids are propagated using a Lie
group variational integrator and its linearization, respectively. The attitude and angular velocity estimates are optimal in the sense that the sizes
of the uncertainty ellipsoids are minimized.},
	annote = {The standard approach for attitude estimation is to represent the measurement noise using probabilistic models and the estimate the attitude using a Bayesian Filter. (Kalman filters and particle filters are different instantiations of Bayesian filters.) An alternative approach is to represent the measurement noise using deterministic bounded uncertainties. Primarily for historical reasons, the bounded uncertainty approach has not been pursued and developed, other than in a few isolated papers. In this paper Dr. Sanyal derives an attitude estimation scheme based on bounded uncertainties, where the attitude is represented by a rotation matrix. The result is impressive in that not only is the algorithm derived, but a sufficient condition is also presented that guarantees asymptotic convergence of the estimation error. This is a new alternative to standard methods based on Bayes filters, and I believe that the importance of this paper will increase with time.},
	author = {Sanyal, Amit K and Lee, Taeyoung and Leok, Melvin and McClamroch, N Harris},
	file = {:Users/beard/Library/Application Support/Mendeley Desktop/Downloaded/Sanyal et al. - 2008 - Global optimal attitude estimation using uncertainty ellipsoids.pdf:pdf},
	journal = {Systems {\&} Control Letters},
	keywords = {Lie group variational integrator,attitude estimation,bounded uncertainties,uncertainty ellipsoid},
	pages = {236--245},
	title = {Global Optimal Attitude Estimation Using Uncertainty Ellipsoids},
	volume = {57},
	year = {2008}}

@inproceedings{SunBeardColton10,
	abstract = {Abstract---Aerial recovery of micro air vehicles (MAVs) presents a
challenging problem in multi-vehicle dynamics and control. This paper
presents a method for recovering MAVs in flight using a mothership
and towed drogue, in which the mothership executes an orbit that
places the drogue in a stable, slower orbit that can be tracked by
a MAV. The differential flatness property of the system is exploited
to calculate mothership trajectories from desired drogue orbits,
and a backstepping controller is proposed that enables accurate mothership
trajectory tracking. Simulation results based on multi-link cable-drogue
systems verify the feasibility and robustness of the approach.},
	address = {Baltimore, MA},
	author = {Sun, Liang and Beard, Randal W and Colton, Mark B},
