	keywords = {BAYESIAN-APPROACH,CLUTTER,EM,INCOMPLETE DATA,PARTICLE FILTERS,SIMULATION,STATE ESTIMATION,SYSTEMS,TARGET TRACKING,TIME-SERIES},
	number = {1},
	pages = {309--332},
	title = {Monte Carlo filtering for multi-target tracking and data association},
