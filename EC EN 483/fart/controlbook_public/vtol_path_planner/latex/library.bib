@inproceedings{KarhoffLimbOravsky06,
	abstract = {The Program Manager of the Army's Unmanned Aerial

Systems (UAS) awarded a contract for the Extended Range

Multipurpose (ERMP) Unmanned Aerial System (UAS) to

