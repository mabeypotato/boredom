	journal = {Journal of Intelligent and Robotic Systems: Theory and Applications},
	keywords = {Autonomous landing,Model predective control,Parameter identification,State estimation,Unnmaned helicopters},
	number = {1},
	pages = {145--158},
	publisher = {Journal of Intelligent {\&} Robotic Systems},
	title = {Vision-based Autonomous Landing Control for Unmanned Helicopters},
