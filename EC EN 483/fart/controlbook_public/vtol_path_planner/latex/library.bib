@article{Leishman2012,
	abstract = {Abstract---This paper discusses the progress made on developing{\$}\backslash{\$}na multi-rotor helicopter equipped with a vision-based{\$}\backslash{\$}nability to navigate through an a priori unknown, GPS-denied{\$}\backslash{\$}nenvironment. We highlight the backbone of our system, the{\$}\backslash{\$}nrelative estimation and control. We depart from the common{\$}\backslash{\$}npractice of using a globally referenced map, preferring instead{\$}\backslash{\$}nto keep the position and yaw states in the EKF relative to the{\$}\backslash{\$}ncurrent map node. This relative navigation approach allows{\$}\backslash{\$}nsimple application of sensor updates, natural characterization{\$}\backslash{\$}nof the transformation between map nodes, and the potential to{\$}\backslash{\$}ngenerate a globally consistent map when desired. The EKF fuses{\$}\backslash{\$}nview matching data from a Microsoft Kinect with more frequent{\$}\backslash{\$}nIMU data to provide state estimates at rates high enough to{\$}\backslash{\$}ncontrol the vehicles fast dynamics. Although an EKF is used, a{\$}\backslash{\$}nnodes and edges graph represents the map. Hardware results{\$}\backslash{\$}nshowing the quality of the estimates and flights with estimates{\$}\backslash{\$}nin the loop are provided.},
