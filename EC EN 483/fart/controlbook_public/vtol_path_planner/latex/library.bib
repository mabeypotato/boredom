@conference{NaldiMarconi10,
	abstract = {This paper focuses on the problem of performinga robust transition maneuver for a particular class of tailsitter V/STOL (Vertical/Short Take-Off and Landing) aerialvehicles in which a switch from the hover flight towards themore efficient level flight configuration can be obtained by achange of the attitude. By considering also the presence ofwind disturbances, this work proposes a control strategy basedupon a path planning approach in order to improve the flightenvelope protection of the airframe.},
	address = {Atlanta, GA},
	annote = {This paper considers the problem of transitioning a tailsitter aircraft between level and hover flight in wind conditions. The focus is on the control strategy and not on planning the transition. The idea is to use a path following approach where the time index of a preplanned path is manipulated to ensure that the vehicle stay within the flight envelop.


