@inproceedings{HuangMourikisRoumeliotis09,
	abstract = {In this work, we study the inconsistency problem of extended Kalman
filter (EKF)-based simultaneous localization and mapping (SLAM) from
the perspective of observability. We analytically prove that when
the Jacobians of the process and measurement models are evaluated
at the latest state estimates during every time step, the linearized
error-state system employed in the EKF has an observable subspace
of dimension higher than that of the actual, non-linear, SLAM system.
As a result, the covariance estimates of the EKF undergo reduction
in directions of the state space where no information is available,
which is a primary cause of the inconsistency. Based on these theoretical
results, we propose a general framework for improving the consistency
of EKF-based SLAM. In this framework, the EKF linearization points
are selected in a way that ensures that the resulting linearized
system model has an observable subspace of appropriate dimension.
We describe two algorithms that are instances of this paradigm. In
the first, termed observability constrained (OC)-EKF, the linearization
points are selected so as to minimize their expected errors (i.e.
the difference between the linearization point and the true state)
under the observability constraints. In the second, the filter Jacobians
are calculated using the first-ever available estimates for all state
variables. This latter approach is termed first-estimates Jacobian
(FEJ)-EKF. The proposed algorithms have been tested both in simulation
and experimentally, and are shown to significantly outperform the
standard EKF both in terms of accuracy and consistency.},
	annote = {This is a very interesting paper that shows that for a mobile robot
with range and bearing estimates, the nonlinear observability analysis
shows that there are three unobservable modes (position and orientation).
However, when the system is linearized to be used in the extended
Kalman filter, the linearized system only has two unobservable modes.
In other words, the linear system thinks that it has more information
then it really has.

The authors postulate that the change in observable modes is what
causes the filter to become inconsistent. They also design two modifications
that select different points to linearize the system, and show that
at those points, observability does not change. With the new modifications,
the filter remains consistent.

A very important paper on SLAM.},
	author = {Huang, Guoquan P and Mourikis, Anastasios I and Roumeliotis, Stergios I},
	booktitle = {The International Journal of Robotic Research},
	keywords = {SLAM,extended Kalman filter,nonlinear estimation,observability},
	month = {apr},
	pages = {502--528},
	title = {Observability-based Rules for Designing Consistent EKF SLAM Estimators},
	volume = {29},
	year = {2010}}

@inproceedings{WilliamsOckels09,
	abstract = {The payload capability of many aircraft is typically limited by the
maximum takeoff weight. However, when in flight, many aircraft can
handle larger loads, but this capability is rarely exploited. The
possibility of using an aircraft cable system to lift large payloads
is studied in this paper. This concept has been studied previously
for single-aircraft, single-cable systems in some detail. The major
limitation of such systems is the need to fly very tight circles
at high-lift coefficients. This limits the size of the payload that
can be retrieved with such a system. To overcome this, it is possible
to employ a multiple-aircraft, multiple-cable system to allow greater
control of the payload position as well as allowing a much wider
choice of aircraft speed and lift coefficient. In this paper, an
object-oriented cable modeling approach is used to study the relative
equilibria of such a system. Parametric studies of different payload
masses and aircraft orbit parameters are presented, and the dynamic
stability is analyzed. Collision-free transition trajectories that
take the multiple-aircraft system from circular flight into straight
and level flight are derived.},
	annote = {This paper addresses the problem of lifting a heavy object using
multiple fixed wing vehicles with towed cables to the object.

The paper gives a detailed analysis of the requirements placed on
the vehicle to be able to lift a payload. The requirements include
lift coefficient, roll angle, thrust, etc.

Also, the paper addresses the fly-in, fly-out proceedure for cooperatively
entering and exiting the orbits.},
	author = {Williams, Paul and Ockels, Wubbo},
	booktitle = {Journal of Guidance, Control, and Dynamics},
	file = {:Users/beard/Library/Application Support/Mendeley Desktop/Downloaded/Williams, Ockels - 2009 - Dynamics of Towed Payload System Using Multiple Fixed-Wing Aircraft.pdf:pdf},
	keywords = {aerial recovery,collision avoidance,cooperative control,towed cable},
