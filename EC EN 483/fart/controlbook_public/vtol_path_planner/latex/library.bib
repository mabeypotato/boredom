	journal = {Robotics and Autonomous Systems},
	keywords = {Joint probabilistic data association, radar, stereo, multiple target tracking},
	month = {May},
	pages = {338--348},
	title = {Radar and Stereo Vision Fusion for Multitarget Tracking on the Special {E}uclidean Group},
	volume = {83},
	year = {2016},
	bdsk-file-1 = {}}

