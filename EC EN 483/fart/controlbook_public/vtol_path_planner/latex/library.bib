@conference{RodriguesTsiogkasAguiar20,
	abstract = {Abstract--- In this paper, we propose a mapping technique that builds a continuous representation of the environment from range data. The strategy presented here encodes the probability of points in space to be occupied using 2.5D Bspline surfaces. For a fast update rate, the surface is recursively updated as new measurements arrive. The proposed B-spline map is less susceptible to precision and interpolation errors that are present in occupancy grid-based methods. From simulation and experimental results, we show that this approach leverages the floating point resolution of continuous metric maps and the fast update/access/merging advantages of discrete metric maps. Thus, the proposed method is suitable for online robotic tasks such as localization and path planning, requiring minor modification to existing software that usually operates on metric maps.},
	address = {Las Vegas, Nevada},
	annote = {This paper shows how to use B-splines to create continuous occupancy maps of the environment.  The log-odds map is modeled using a B-spline surface that is updated using range measurements from a sensor.  The position of the robot is assumed to be known perfectly.

The overview of spline surface is really nice, and the overview of the log-odds update method is nice.  },
	author = {Romulo T. Rodrigues and Nikolaos Tsiogkas and A. Pedro Aguiar and Antonio Pascoal},
	booktitle = {Proceedings of the {IEEE/RSJ} International Conference on Intelligent Robots and System},
	date-added = {2022-05-03 15:27:50 -0600},
	date-modified = {2022-05-03 15:30:34 -0600},
	keywords = {B-spline surfaces, occupancy maps,},
	month = {October},
