@conference{CaseyKarpenkoCurry13,
	abstract = {Complementary lter-based algorithms for attitude estimation seek to minimize the
short-comings associated with the MEMS sensors and embedded processors that are fre-
quently used in low-cost unmanned aerial vehicles. Within the complementary lter frame-
work, this paper explores the performance impact of three dierent aspects of attitude
estimation: 1) kinematic propagation, 2) attitude representation, and 3) the assumptions
on the behavior of the angular rate vector between samples. Forward Euler integration
and matrix exponential propagation methods are compared within several attitude rep-
resentations, namely the direction cosine matrix (DCM), quaternions, Euler angles, and
the angle-axis scheme. Assumptions of constant, linear, and quadratic angular velocities
between samples are also examined. The resulting algorithms are evaluated according to
accuracy, computational load, and noise response. Unit tests using analytic inputs and
integration tests using each approach within the complementary lter framework of the
SLUGS autopilot are performed to evaluate algorithm performance against 
ight test data
from an integrated GPS/INS system. The results of the experiments indicate that the
matrix exponential with quaternion attitude representation delivers higher accuracy with
low penalty on the computational burden and no signicant dierence for noise resilience.},
	annote = {This paper compares different methods for representing the attitude, and different methods for propagating the attitude given measurements of the angular velocity vector.

The method that provides the best tradeoff in speed and accuracy is to to represent the attitude with a quaternion, and to propagate using the exponential matrix and to interpolate the angular velocity vector using a quadratic interpolation.},
	author = {Casey, Robert T and Karpenko, Mark and Curry, Renwick and Elkaim, Gabriel},
	booktitle = {AIAA Guidance, Navigation, and Control (GNC) Conference, Guidance, Navigation, and Control},
	file = {:Users/beard/Dropbox/library/CaseyKarpenkoCurry13-1.pdf:pdf},
	title = {Attitude Representation and Kinematic Propagation for Low-Cost UAVs},
	year = {2013}}

@article{Agha-mohammadi2014b,
	abstract = {In this paper we present feedback-based information roadmap (FIRM), a multi-query approach for planning under uncertainty which is a belief-space variant of probabilistic roadmap methods. The crucial feature of FIRM is that the costs associated with the edges are independent of each other, and in this sense it is the first method that generates a graph in belief space that preserves the optimal substructure property. From a practical point of view, FIRM is a robust and reliable planning framework. It is robust since the solution is a feedback and there is no need for expensive replanning. It is reliable because accurate collision probabilities can be computed along the edges. In addition, FIRM is a scalable framework, where the complexity of planning with FIRM is a constant multiplier of the complexity of planning with PRM. In this paper, FIRM is introduced as an abstract framework. As a concrete instantiation of FIRM, we adopt stationary linear quadratic Gaussian (SLQG) controllers as belief stabilizers and introduce the so-called SLQG-FIRM. In SLQG-FIRM we focus on kinematic systems and then extend to dynamical systems by sampling in the equilibrium space. We investigate the performance of SLQG-FIRM in different scenarios.},
	author = {Agha-mohammadi, Ali Akbar and Chakravorty, Suman and Amato, Nancy M.},
	doi = {10.1177/0278364913501564},
	file = {:Users/beard/Dropbox/library/AghamohammadiChakravortyAmato14.pdf:pdf},
	issn = {17413176},
	journal = {International Journal of Robotics Research},
	keywords = {-Belief space,Control,Information,Planning,Uncertainty},
	number = {2},
	pages = {268--304},
	title = {FIRM: Sampling-based feedback motion-planning under motion uncertainty and imperfect measurements},
	volume = {33},
	year = {2014},
	bdsk-url-1 = {https://doi.org/10.1177/0278364913501564}}

