@article{PongpunwattanaRysdyk04,
	abstract = {This paper describes a combined task planning and path planning solution
for UAVs that operate in dynamic and uncertain environments. The
paper gives an excellent literature review of single vehicle planning
and multi-vehicle planning. A large portion of the paper is devoted
