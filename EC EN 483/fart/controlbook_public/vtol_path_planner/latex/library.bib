@article{GripFossenJohansenSaberi14,
	abstract = {This paper deals with the construction of nonlinear observers for navigation purposes. We first present a globally exponentially stable observer for attitude and gyro bias, based on gyro measurements and two or more pairs of vector measurements. We avoid the well-known topological obstructions to global stability by not confining the attitude estimate to SO(3), but rather estimating a full rotation matrix with nine degrees of freedom. We also show how the attitude can be estimated under a relaxed persistency-of-excitation condition, with a single vector measurement as a special case. Next, we use the attitude observer to construct a globally exponentially stable observer for gnss/ins integration, based on accelerometer, gyro, and magnetometer measurements, as well as gnss measurements of position and (optionally) velocity. We verify the stability properties of the design using experimental data from a light fixed-wing aircraft.},
	annote = {This paper derives a globally asymptotically stable observer for attitude.  The paper includes an observer for R but does not force R to stay on SO(3), but allows it to range over all of R^{3x3}.  Two time varying attitude references are required for the paper.  In particular, the attitude references could be gravity and magnetic field.  The gyro biases are also estimated.  The attitude estimator is also used in a larger GPS corrected filter to get position and velocity.},
	author = {Havard Fjaer Grip and Thor I. Fossen and Tor A. Johansen and Ali Saberi},
	date-added = {2017-08-10 18:03:14 +0000},
	date-modified = {2018-10-13 10:27:49 -0500},
	journal = {Automatica},
	keywords = {Guidance, navigation, and control of vehicles; Nonlinear observer and filter design},
	month = {January},
	pages = {158-166},
	title = {Globally Exponentially Stable Attitude and Gyro Bias Estimation with Application to GNSS/INS Integration},
	volume = {51},
	year = {2015},
	bdsk-file-1 = {}}

@article{BatistaSilvestreOliveira13,
	abstract = {This paper presents a set of filters with globally exponentially stable error dynamics for source localization
