	title = {Radar and stereo vision fusion for multitarget tracking on the special Euclidean group},
	url = {http://dx.doi.org/10.1016/j.robot.2016.05.001},
	volume = {83},
	year = {2016},
	bdsk-url-1 = {http://dx.doi.org/10.1016/j.robot.2016.05.001}}

