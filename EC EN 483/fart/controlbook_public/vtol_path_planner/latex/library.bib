@article{RaihanChakravorty18,
	abstract = {Recursive estimation of nonlinear dynamical systems is an important problem that arises in several engineering applications. It
is well known that the posterior state estimates in nonlinear problems may assume non-Gaussian multimodal densities. In the
past, Gaussian mixture lters and particle lters were introduced to handle non-Gaussianity and nonlinearity. However, these
methods have found only limited success as most Gaussian mixture lters attempt to x the number of mixture modes during
the estimation process, and the particle lters suer from the curse of dimensionality. In this paper, we propose a particle
based Gaussian mixture ltering approach for nonlinear estimation that is free of the particle depletion problem inherent to
most particle lters. We employ an ensemble of possible state realizations for the propagation of state probability density. A
Gaussian mixture model (GMM) of the propagated uncertainty is then recovered by clustering the ensemble. The posterior
density is obtained subsequently through a Kalman measurement update of the mixture modes. We prove the convergence
in probability of the resultant density to the true lter density assuming exponential forgetting of initial conditions by the
true lter. The performance of the proposed ltering approach is demonstrated through several test cases and is extensively
compared to other nonlinear lters. The particle Gaussian mixture lter is capable of handling the non-Gaussianity of the state
pdf arising from dynamics, initial conditions or process noise. A more general approach that can also handle non-Gaussianity
related to measurement update is presented in a companion paper.},
	author = {Dilshad Raihan and Suman Chakravorty},
	date-added = {2018-08-30 13:00:50 -0600},
	date-modified = {2018-08-30 13:06:47 -0600},
	journal = {arXiv:1603.04510},
	title = {Particle Gaussian Mixture Filters-I},
	year = {2016},
	bdsk-file-1 = {}}

